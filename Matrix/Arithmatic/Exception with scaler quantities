>> A = [1 2 3;4 5 6]

A =

     1     2     3
     4     5     6

>> A+3

ans =

     4     5     6
     7     8     9

>> 3+A

ans =

     4     5     6
     7     8     9

>> A-1

ans =

     0     1     2
     3     4     5

>> 2.*A

ans =

     2     4     6
     8    10    12

>> 2*A

ans =

     2     4     6
     8    10    12

>> A./2

ans =

    0.5000    1.0000    1.5000
    2.0000    2.5000    3.0000

>> A.\2

ans =

    2.0000    1.0000    0.6667
    0.5000    0.4000    0.3333

>> A.^2

ans =

     1     4     9
    16    25    36

>> 2.^A

ans =

     2     4     8
    16    32    64

>> A

A =

     1     2     3
     4     5     6

>> 
