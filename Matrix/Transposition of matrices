>> H = [1 2 3;4 5 6]

H =

     1     2     3
     4     5     6

>> G = H'

G =

     1     4
     2     5
     3     6


>> X = [1;3;5;7]

X =

     1
     3
     5
     7

>> Y = X'

Y =

     1     3     5     7
     
    

>> 1:2:5

ans =

     1     3     5
      
>> (1 : 2 : 5)'

ans =

     1
     3
     5
    
