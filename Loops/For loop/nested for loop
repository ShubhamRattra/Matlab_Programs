#Editor

[row col] = size(A)
 for r = 1:row
     for c = 1:col
         P(r,c) = A(r,c) * A(r,c);
     end
 end
 
 
 #Command window
 
 >> A = randi(10,3,4)

A =

     2     8     1     9
    10     9     4     5
     1     9     3    10

>> mul

row =

     3


col =

     4

>> P

P =

     4    64     1    81
   100    81    16    25
     1    81     9   100

>> 
